generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Collection {
  id           String @id @default(cuid())
  anime_mal_id String
  user_email   String

  @@unique([anime_mal_id, user_email])
  @@map("collections")
}
